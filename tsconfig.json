{
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@source/*": [
        "./src/source/*"
      ]
    }
  },
  "target": "ES2020",
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "src/localization/translations",
    "src/localization/types/i18n.d.ts",
    "src/helpers/log/react-native-console-table/modules/dataForTable.ts",
    "src/types/**/*.d.ts"
  ],
  "extends": "expo/tsconfig.base"
}
